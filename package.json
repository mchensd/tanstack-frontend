{
  "name": "frontend-v2",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "24.12.0"
  },
  "scripts": {
    "dev": "yarn typecheck && nodemon server-dev.ts",
    "preview": "yarn typecheck && nodemon server-prod.ts",
    "typecheck": "tsc",
    "build:all": "yarn typecheck && yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx"
  },
  "dependencies": {
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "express": "^5.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2"
  }
}
